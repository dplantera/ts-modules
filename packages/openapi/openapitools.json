{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.1.0",
    "generators": {
      "ts-types": {
        "inputSpec": "specs/pets-api.yml",
        "additionalProperties": {
          "enumUnknownDefaultCase": true
        },
        "output": "dist",
        "generatorName": "typescript-axios",
        "modelNamePrefix": "Dto"
      }
    }
  }
}
